
<h2>Category List</h2>
<div class="render_data"></div>

<script type="text/x-handlebars-template" id="category_list">
    @* <button type="button" class="btn btn-danger float-right" onclick="DeleteSelectedPrizebond()">Delete Selected</button> *@
        <table class="table table-hover">
            <tr>
                <th>Category Id</th>
                <th>Name</th>
                <th>Action</th>
            </tr>
            {{#each rows}}
            <tr>
                <td>{{id}}</td>
                <td>{{categoryName}}</td>
                  <td>
                      <button type="button" onclick="Edit({{id}})" class="btn btn-warning">Edit</button>

                      <button type="button" onclick="window.location.href='catego/'+{{id}}" class="btn btn-danger">Edit</button>
                   </td>
            </tr>
            {{/each}}
        </table>
</script>

<script src="~/js/Category.js"></script>